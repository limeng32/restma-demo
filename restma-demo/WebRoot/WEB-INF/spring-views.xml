<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="  
        http://www.springframework.org/schema/beans   
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	<bean id="userListExcel"
		class="cn.limeng32.testSpring.web.UserListExcelView" />
	<bean id="userListPdf"
		class="cn.limeng32.testSpring.web.UserListPdfView" />
	<bean id="showXml"
		class="org.springframework.web.servlet.view.xml.MarshallingView">
		<property name="marshaller" ref="xmlMarshaller"></property>
		<property name="modelKey" value="_content"></property>
	</bean>
	<bean id="showJson"
		class="org.springframework.web.servlet.view.json.MappingJackson2JsonView">
		<property name="modelKeys" value="_content"></property>
	</bean>
</beans>